{
  "manifest_version": 3,
  "name": "Zillow to Google Maps",
  "version": "1.0.0",
  "description": "Open Zillow house listing on Google Maps",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "content_scripts": [
    {
      "matches": ["*://www.zillow.com/*"],
      "js": ["content.js"]
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
